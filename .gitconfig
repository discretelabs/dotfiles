[user]
	name = Your Name
	email = Your Email Address
	net = Your Name
[color]
	ui = true

[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse

[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold

[core]
	editor = nano
	excludesfile = /Users/[username]/.gitignore_global
	whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = ~/.gitignore

[difftool "atom"]
	prompt = false
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = /usr/local/bin/atom #/usr/bin/vimdiff 

[push]
	default = simple

[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f

[alias]
	a = add
	s = status
	sb = status -s -b
	c = commit -v
	b = branch
	m = merge
	r = rebase
	f = fetch
	pull = pull -ff-only
	push = push
	diff = diff --color-words -w
	diffs = diff --staged
	log = log
	#logg = log --graph --decorate --oneline --abbrev-commit --all
	logg = log --decorate --oneline --abbrev-commit --all -n 20
	co = checkout
	rs = remote show
	last = log -1 HEAD
	untrack = rm --cached
	unstage = reset HEAD --
